{
  "plugins": ["import"],
  "extends": ["eslint:recommended", "prettier", "prettier/react"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error",
    "import/first": "error"
  },
  "env": {
    "browser": true,
    "es6": true
  }
}